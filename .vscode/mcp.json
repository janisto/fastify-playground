{
	"servers": {
		"github": {
			"type": "http",
			"url": "https://api.githubcopilot.com/mcp/"
		},
		"firebase": {
			"type": "stdio",
			"command": "npx",
			"args": ["-y", "firebase-tools@latest", "experimental:mcp"]
		},
		"context7": {
			"type": "stdio",
			"command": "npx",
			"args": ["-y", "@upstash/context7-mcp"]
		}
	}
}
